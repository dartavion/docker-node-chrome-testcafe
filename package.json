  "scripts": {
    "test:chrome": "cucumber-js --world-parameters {\\\"browsers\\\":[\\\"chrome\\\"]\\,\\\"exampleUrl\\\":\\\"$npm_package_testCafeExamplePageUrl\\\"\\,\\\"comsystoReplyUrl\\\":\\\"$npm_package_comsystoReplyUrl\\\"\\} --format json:reports/Chrome.json",
    "build:browsers": "docker build -t browsers -f docker/browsers/Dockerfile .",
    "generateHtmlReport": "rm -rf reports/combined && node src/reporters/html/prepareJson.js && node src/reporters/html/generateHtml.js",
    "test-chrome": "./node_modules/.bin/cucumber-js  --world-parameters \"{\\\"browser\\\": \\\"chrome:headless\\\"}\" --format json:reports/Chrome.json",
    "e2e": "mkdir -p reports; npm run test-chrome; npm run generateHtmlReport"
  },
